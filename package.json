	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js",
		"test:coverage": "node scripts/test.js --coverage",
		"test:visual": "backstop test --docker --config=config/backstop/backstop.config.js",
		"test:visual:approve": "backstop approve --config=config/backstop/backstop.config.js",
		"lint": "eslint --ext .js,.jsx src scripts config",
		"lint:fix": "eslint --ext .js,.jsx src scripts config --fix",
		"lint:watch": "esw -w --clear --ext .js,.jsx src scripts config",
		"serve:build": "http-server build",
		"generatedocs": "documentation build src/** -f md -o docs/typedefs.md",
		"style:lint": "stylelint 'src/**/*.css'",
		"style:fix": "stylelint 'src/**/*.css' --fix",
		"translate": "node --inspect scripts/translate.js "
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@google-cloud/translate": "^4.1.3",
		"backstopjs": "4.0.3",
		"coveralls": "^3.0.3",
		"documentation": "^10.1.0",
		"enzyme": "^3.10.0",
		"enzyme-to-json": "^3.4.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^22.4.1",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-react-hooks": "^1.6.0",
		"eslint-utils": "^1.4.2",
		"eslint-watch": "^5.0.1",
		"fetch-mock": "^7.4.0",
		"http-server": "^0.11.1",
		"husky": "^1.3.1",
		"jest": "^24.9.0",
		"lint-staged": "^8.1.5",
		"prettier": "^1.17.0",
		"redux-mock-store": "^1.5.3",
		"stylelint": "^10.1.0",
		"stylelint-config-recommended": "^2.2.0",
		"unused-files-webpack-plugin": "^3.4.0",
		"waait": "^1.0.5",
		"worker-loader": "^2.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run generatedocs && git add docs/typedefs.md && lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom",
			"<rootDir>/config/jest/enzymeSetup.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
			"^.+\\.module\\.(css)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
			"^file-loader.*\\!papaparse$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"<rootDir>/node_modules/jest-watch-typeahead/filename.js",
			"<rootDir>/node_modules/jest-watch-typeahead/testname.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
